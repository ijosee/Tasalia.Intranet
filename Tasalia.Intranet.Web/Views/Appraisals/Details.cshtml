@model Tasalia.Intranet.DataBase.Models.Appraisal

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0 text-dark">Detalle de la ciudad con id @Model.AppraisalId</h1>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Detalle</h3>
                            <div class="card-tools">
                                <!-- Buttons, labels, and many other things can be placed here! -->
                                <!-- Here is a label for example -->
                                <a asp-action="Edit" asp-route-id="@Model.AppraisalId">Editar</a> | <a asp-action="Index">Volver a la lista</a>
                            </div>
                        </div>
                        <div class="card-body">
                            <dl class="row">
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.ProcessGuid)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.ProcessGuid)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Number)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Number)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Supplement)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Supplement)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.CreateDate)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.CreateDate)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.UpdateDate)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.UpdateDate)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Deputation)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Deputation.DeputationId)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Property)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Property.PropertyId)
                                </dd>
                            </dl>
                        </div>
                        <div class="card-footer">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>
